[remote "martinsoft"]
    url = git@github.com:martinsoft/wifi-auto-reconnect.git
    fetch = +refs/heads/*:refs/remotes/martinsoft/*
[remote "origin"]
    url = git@github.com:huangyingw/martinsoft_wifi-auto-reconnect.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = martinsoft
    target = martinsoft/uninstall
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[deploy]
	host = localhost
	path = ~/myproject/git/tools/martinsoft/wifi-auto-reconnect
